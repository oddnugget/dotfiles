# vim: set ft=zsh

alias ncfg='nvim --cmd "cd $HOME/.config/nvim"'
alias ls='exa -1 -F --icons $1'
alias dot='cd $HOME/dotfiles'
alias cat='bat $1'
alias devssh='kitten ssh devbox'
alias fugit="nvim -c \"G\" -c \"only\""

function psg() { 
  ps aux | grep $1 | sed G
}

alias zsrc="source $HOME/.zshrc"

